{
  "name": "mtc-repertoire-engine",
  "productName": "mtc-repertoire-engine",
  "version": "1.0.0",
  "description": "Application to load and use local chess engines as external engine for Mind The Check Openings Repertoire",
  "main": ".vite/build/main.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxqua72/mtc-repertoire-engine.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "set VITE_CJS_TRACE=true && electron-forge make --verbose",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "postbuild": "cp ./src/main/server.js ./.vite/build/server.js && cp ./src/main/updater.js ./.vite/build/updater.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron-forge/plugin-vite": "^7.6.0",
    "@electron/fuses": "^1.8.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "33.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "vite": "^5.4.11"
  },
  "keywords": [],
  "author": {
    "name": "maxqua72",
    "email": "maxqua72@gmail.com"
  },
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "bootstrap": "^5.3.3",
    "chess.js": "^1.0.0-beta.4",
    "chessground": "^8.4.0",
    "dotenv": "^16.4.7",
    "electron-log": "^5.2.4",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.0",
    "pinia": "^2.0.28",
    "uuid": "^11.0.3",
    "vue": "^3.5.13",
    "ws": "^8.18.0"
  }
}
